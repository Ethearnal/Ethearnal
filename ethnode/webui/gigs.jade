        // Semantic UI JS
        script(src="https://cdn.rawgit.com/Semantic-Org/Semantic-UI-CSS/master/semantic.min.js")

        // Moment.JS
        script(src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.17.1/moment.min.js")

        // Bootstrap Date Time Picker
        script(src="https://cdn.rawgit.com/T00rk/bootstrap-material-datetimepicker/gh-pages/js/bootstrap-material-datetimepicker.js")

        // Moment JS
        script(src="https://cdn.jsdelivr.net/npm/moment@2.19.1/moment.min.js")

        // Material Design Bootstrap
        script(defer src="https://code.getmdl.io/1.3.0/material.min.js")

        // Jquery Easing
        script(src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js")

        // loadProfile.js
        script(src="js/loadProfile.js")

